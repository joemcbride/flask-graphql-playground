# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Editor default newlines with a newline ending every file
[*]
insert_final_newline = true
charset = utf-8
indent_style = space
indent_size = 2
trim_trailing_whitespace = true

[*.html]
indent_size = 2

[*.py]
indent_size = 4
